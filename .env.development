# Database
DATABASE_URL=postgresql://postgres:postgres@db:5432/freelance_dashboard

# Redis
REDIS_URL=redis://redis:6379

# Next.js
NEXT_PUBLIC_API_URL=http://localhost:3000/api

# Authentication
JWT_SECRET=your-jwt-secret-key
JWT_EXPIRES_IN=7d

# Form Settings
FORM_AUTO_SAVE=true
FORM_SAVE_INTERVAL=5000
FORM_TRACK_HISTORY=true

# Feature Flags
ENABLE_ANALYTICS=false
ENABLE_LOGGING=true
ENABLE_CACHE=true

# Development Settings
NODE_ENV=development
DEBUG=true 